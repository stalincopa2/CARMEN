@model IEnumerable<CARMEN.Models.OBSERVACION>

@{
    ViewBag.Title = "Index";
}

<h2>Listado de observaciones</h2>

<p>
    @Html.ActionLink("Crear Observacion", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DET_OBSER)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DET_OBSER)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID_OBSERVACION }) |
            @Html.ActionLink("Detalle", "Details", new { id=item.ID_OBSERVACION }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.ID_OBSERVACION })
        </td>
    </tr>
}

</table>
